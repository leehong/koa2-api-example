{
  "name": "koa2-api-example",
  "version": "1.0.0",
  "description": "koa2 example write api",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/leehong/koa2-api-example.git",
    "type": "git"
  },
  "author": "leron",
  "license": "MIT",
  "scripts": {
    "console": "node index.js console --harmony",
    "start": "./node_modules/.bin/nodemon index.js",
    "app": "node index.js",
    "pm2": "NODE_ENV=production ./node_modules/.bin/pm2 start index.js -i 2 --name \"hearteye\" --max-memory-restart 300M --merge-logs --log-date-format=\"YYYY-MM-DD HH:mm Z\" --output=\"log/production.log\"",
    "pm2:restart": "NODE_ENV=production ./node_modules/.bin/pm2 restart \"hearteye\"",
    "pm2:stop": "NODE_ENV=production ./node_modules/.bin/pm2 stop \"hearteye\"",
    "pm2:monit": "NODE_ENV=production ./node_modules/.bin/pm2 monit \"hearteye\"",
    "pm2:logs": "NODE_ENV=production ./node_modules/.bin/pm2 logs \"hearteye\"",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive --harmony --require babel-polyfill --timeout 80000 test/",
    "lint": "eslint . --ext .js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "create:migration": "node_modules/.bin/sequelize migration:create"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^0.8.7",
    "koa": "2",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^2.5.0",
    "koa-generic-session": "^1.11.3",
    "koa-json": "^1.1.1",
    "koa-logger": "^2.0.0",
    "koa-methodoverride": "^2.0.0",
    "koa-redis": "^2.1.1",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.2",
    "lodash": "^4.16.6",
    "moment": "^2.14.1",
    "path": "^0.12.7",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "pm2": "^1.1.3",
    "sequelize": "^3.26.0",
    "sequelize-cli": "^2.4.0",
    "xss": "^0.2.13"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.7",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "eslint-config-volox": "^1.0.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.2",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "supertest": "^1.2.0"
  }
}
